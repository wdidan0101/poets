<h1>Sign Up!</h1>

<%= form_for @user do |f| %>
     <%= f.label :name %>
     <%= f.text_field :name %>

     <%= f.label :email %>
     <%= f.text_field :email %>

     <%= f.label :origin %>
     <%= f.text_field :origin %>

     <%= f.label :password %>
     <%= f.text_field :password %>

     <%= f.submit "Sign Me Up!" %>
<% end %>