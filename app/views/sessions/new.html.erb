<h1>Log In.</h1>

<%= form_for(:session, url: sessions_path) do |f| %>

	<div>
    <%= f.label :email %> <%= f.text_field :email %>
	</div>
	<div>
    <%= f.label :password %> <%= f.password_field :password %>
	</div>
    
    <%= f.submit "Sign In!" %>
<% end %>

<%= link_to "New User.", signup_path %>